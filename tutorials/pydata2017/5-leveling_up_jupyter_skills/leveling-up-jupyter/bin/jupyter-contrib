#!/home/donal/cyberdyne/pydata2017/leveling_up_jupyter_skills/leveling-up-jupyter/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'jupyter-contrib-core==0.3.1','console_scripts','jupyter-contrib'
__requires__ = 'jupyter-contrib-core==0.3.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jupyter-contrib-core==0.3.1', 'console_scripts', 'jupyter-contrib')()
    )
